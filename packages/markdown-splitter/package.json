{
  "name": "@growi/markdown-splitter",
  "version": "1.0.0",
  "license": "MIT",
  "private": "true",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "clean": "shx rm -rf dist",
