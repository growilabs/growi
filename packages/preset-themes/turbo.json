{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": ["@growi/core#build"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "dev": {
      "dependsOn": ["@growi/core#dev"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "watch": {
      "dependsOn": ["@growi/core#dev"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "lint": {
      "dependsOn": ["@growi/core#dev"]
    }
  }
}
